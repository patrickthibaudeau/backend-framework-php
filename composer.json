{
    "name": "devframework/core",
    "description": "A modular PHP framework for secure business applications",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.4",
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.5",
        "mustache/mustache": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "DevFramework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevFramework\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
